{
  "extends": [ "plugin:angular/johnpapa", "standard", "eslint:recommended" ],
  "env": {
    "browser": 1
  },
  "globals": {
    "angular": 1,
    "$": 1,
    "ej": 1,
    "esri":  1,
	"alasql": 1,
	"dijit": 1,
	"dojo": 1
  },
  "rules": {
    "indent": ["error", 2, { "ignoreComments": true }],
	"angular/file-name": 0,
	"angular/controller-name": 0,
  "angular/function-type": [2,"named"],
  "angular/no-service-method": [2, "named"]
  }
}